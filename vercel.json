{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "env": {
    "NODE_ENV": "production",
    "SKIP_TYPESCRIPT_CHECK": "true",
    "NEXT_PUBLIC_SKIP_TYPE_CHECK": "true"
  },
  "outputDirectory": ".next",
  "functions": {
    "app/api/**/*": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
